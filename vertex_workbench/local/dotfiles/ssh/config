# This is an example of what the ssh config entries should look like for ssh into a vertex workbench instance
Host jndev
    HostName jndev.us-west1-a.c.sdp-stg-financial-services.internal
    User jonathan_norton_shopify_com
    IdentityFile ~/.ssh/google_compute_engine
    ProxyCommand gcloud compute start-iap-tunnel jndev 22 --listen-on-stdin --project=sdp-stg-financial-services --zone=us-west1-a --verbosity=warning
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null
    ServerAliveInterval 180
    TCPKeepAlive yes
